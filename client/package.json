{
  "name": "demirkiran-cafe-client",
  "version": "1.0.0",
  "description": "DemirkiranCAFE Desktop Client - Real-time communication app",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron\"",
    "electron:build": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "react",
    "webrtc",
    "mediasoup",
    "streaming"
  ],
  "author": "Dmrkrn",
  "license": "MIT",
  "type": "commonjs",
  "build": {
    "appId": "com.demirkiran.cafe",
    "productName": "DemirkiranCAFE",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "mediasoup-client": "^3.18.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  }
}